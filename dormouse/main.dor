@define HELLO int

// main function - shows examples of syntax highlights
void main(int x, int y) {
    for int i: 0, y, 1 { // for loop - for TYPE VAR: start, end, increment
        if i as float == 1.0 { // casting i to float
            print("It was 1.0")
            continue // goes to next iteration of for loop
        } print(i) // this chunk ON THE SAME LINE as the closing brace of an if, for, or while is the 'else' block, similar to python, it 
                   // executes if the block does not run fully (break in a loop, or if condition not matched). you could also just do 'else'
                   // for multiline blocks
        
        
    }
}



